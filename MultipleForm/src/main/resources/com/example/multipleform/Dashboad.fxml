<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.multipleform.Dashboad">
   <children>
      <Pane layoutX="5.0" layoutY="26.0" prefHeight="360.0" prefWidth="589.0">
         <children>
            <Button fx:id="btnExit" layoutX="75.0" layoutY="249.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
            <Button fx:id="btnLogin" layoutX="251.0" layoutY="249.0" mnemonicParsing="false" onAction="#login" text="Login" />
         </children>
      </Pane>
      <MenuBar layoutX="27.0" layoutY="10.0" prefHeight="32.0" prefWidth="517.0">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <Menu mnemonicParsing="false" text="colors">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Red" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="green" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Black" />
                    </items>
                  </Menu>
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="AboutUS" mnemonicParsing="false" onAction="#Alart" text="About US" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</AnchorPane>
